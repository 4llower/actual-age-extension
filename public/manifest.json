{
  "manifest_version": 2,
  "name": "Actual Age Tab",
  "version": "1.0.1",
  "description": "Extension for displaying your actual age (float number)",
  "icons": {
    "16": "icons/icon_128.png",
    "32": "icons/icon_128.png",
    "48": "icons/icon_128.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Actual Age-extension",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": ["storage"]
}
